generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(cuid())
  username    String   @unique
  displayName String
  email       String   @unique
  role        String   @default("user")
  pin         String?
  password    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
